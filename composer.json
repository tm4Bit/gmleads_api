{
	"name": "ovlk/zapbot",
	"require": {
		"slim/slim": "^4.14",
		"slim/psr7": "^1.7",
		"slim/csrf": "^1.5",
		"guzzlehttp/guzzle": "^7.9",
		"monolog/monolog": "^3.9",
		"php-di/php-di": "^7.0",
		"symfony/var-dumper": "^7.3"
	},
	"autoload": {
		"psr-4": {
			"Ovlk\\GMLeads\\": "src/",
			"Core\\": "Core/",
			"Http\\": "Http/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "Tiago Araujo",
			"email": "tiagomuniz130@gmail.com"
		}
	],
	"require-dev": {
		"pestphp/pest": "^3.8",
		"mockery/mockery": "^1.6",
		"pestphp/pest-plugin-stressless": "^3.1"
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	}
}
